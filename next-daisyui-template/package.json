{
    "name": "next-daisyui-template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "biome lint --write",
        "format": "biome format --write",
        "check": "biome check --write ",
        "type": "tsc --noEmit",
        "ci": "biome ci",
        "test": "vitest run --reporter=dot",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:ci": "vitest run --coverage --reporter=junit --outputFile=junit-report.xml",
        "docs": "typedoc --plugin typedoc-plugin-markdown --entryPointStrategy expand app/"
    },
    "dependencies": {
        "next": "16.1.0",
        "react": "19.2.3",
        "react-dom": "19.2.3"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.6",
        "@tailwindcss/postcss": "^4",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.1",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "globals": "^16.5.0",
        "happy-dom": "^20.0.11",
        "tailwindcss": "^4",
        "typedoc": "^0.28.15",
        "typedoc-plugin-markdown": "^4.9.0",
        "typescript": "^5",
        "vite-tsconfig-paths": "^6.0.3",
        "vitest": "^4.0.16"
    }
}
