        "serena-app": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "--no-env-file",
                "--from",
                "git+https://github.com/oraios/serena",
                "serena",
                "start-mcp-server",
                "--context",
                "ide-assistant",
                "--project",
                "./app"
            ],
            "env": {
                "BROWSER": "none"
            }
        },
        "serena-api": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "--no-env-file",
                "--from",
                "git+https://github.com/oraios/serena",
                "serena",
                "start-mcp-server",
                "--context",
                "ide-assistant",
                "--project",
                "./api"
            ],
            "env": {
                "BROWSER": "none"
            }
        },
        "gitingest-mcp": {
            // GitHubリポジトリをLLM向けテキストに変換
            "type": "stdio",
            "command": "uvx",
            "args": [
                "--no-env-file",
                "git+https://github.com/puravparab/gitingest-mcp",
                "gitingest-mcp"
            ]
        },
        "deepwiki": {
            // GitHub向けDeep Research
            "type": "stdio",
            "command": "npx",
            "args": [
                "mcp-remote",
                "https://mcp.deepwiki.com/sse"
            ]
        },
